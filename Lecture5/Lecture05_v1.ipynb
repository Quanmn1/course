    "#We are not going to plot the fit first, lets just use our barrage of statistics to check if its ok\n",
    "def hubble(z,Om):\n",
    "    pVal=Om*(1+z)**3+(1.-Om)\n",
    "    return np.sqrt(pVal)\n",
    "\n",
    "def lumidistance(x,h0,Om):\n",
    "    integral=0\n",
    "    nint=100\n",
    "    for i0 in range(nint):\n",
    "        z=x*float(i0)/100.\n",
    "        dz=x/float(nint)\n",
    "        pVal=1./(1e-5+hubble(z,Om))\n",
    "        integral += pVal*dz\n",
    "    d=(1.+x)*integral*(1e6*3e5/h0)\n",
    "    return d\n",
    "\n",
    "def loglike(x):\n",
